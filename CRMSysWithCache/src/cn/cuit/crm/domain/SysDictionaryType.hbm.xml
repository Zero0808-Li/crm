<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.cuit.crm.domain.SysDictionaryType" table="sys_dictionary_type">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="sort" type="integer">
			<column name="sort" />
		</property>
		<property name="code" type="string">
			<column name="code" sql-type="varchar(100)" />
		</property>
		<property name="remark" type="text">
			<column name="remark" />
		</property>
		<property name="value" type="string">
			<column name="value" sql-type="varchar(200)" />
		</property>
		<property name="sysFlag" type="character">
			<column name="sysFlag" length="1" />
		</property>
	</class>
</hibernate-mapping>
